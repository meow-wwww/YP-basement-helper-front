<!--pages/appointment/appointment.wxml-->
<view class="weui-cells__title">地下室房间列表</view>

<view class="weui-cells weui-cells_after-title">

<view wx:for="{{namelist.data}}">
  <view class="weui-cell weui-cell_access" hover-class="weui-cell_active" bindtap="navigateroom" data-paraname="{{item.Rid+item.Rtitle}}">
    <view class="weui-cell__bd">{{item.Rid}} {{item.Rtitle}}</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </view>
</view>

</view>